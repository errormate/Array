#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int z;
cin>>z;
for(int j=0;j<z;j++)
{
long long int s=0,cand;
int n;
cin>>n>>cand;
int arr[n];
for(int i=0;i<n;i++)
{
cin>>arr[i];
}
for(int i=0;i<n;i++)
{
s = s+arr[i];
}
if(s <= cand)
{
cout<<"Yes\n";
}
else
{
cout<<"No\n";
}
}


	return 0;
}
